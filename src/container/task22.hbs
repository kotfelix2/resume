<main>
  {{#with page_header}}
    <h1 class='text-center'>{{title}} </h1>
    <p class='text-center'>{{description}} </p>
  {{/with}}
  {{#with section}}
    <div class='card'>
      <div class='card-body'>
        <div
          class='display-6 text-end'
        >{{about_us.title_list.main}} </div>
        <h2 class='text-end'>{{about_us.title_list.sub}}
        </h2>
        <button
          type='button'
          class='btn btn-outline-info'
        >{{about_us.button.text}} </button>
      </div>
    </div>
    <br />

    <div class='card'>
      <div class='card-body'>
        {{#with advantage}}
          <div class='display-5 text-muted'>{{title}} </div>
          <div class='card'>
            {{#with efficiency}}
              <div class='card-body'>
                <div class='display-4 text-muted'>{{title}}
                </div>
                <h6>{{sub_title}} </h6>
                <p>{{description}} </p>
                <button
                  type='button'
                  class='btn btn-outline-warning'
                >{{button}} </button>
              </div>
            {{/with}}
          </div>
          <br />
          <div class='card'>
            {{#with innovation}}
              <div class='card-body'>
                <div
                  class='display-4 text-muted text-center'
                >{{title}}
                </div>
                <h6 class='text-center'>{{sub_title}} </h6>
                <p class='text-center'>{{description}} </p>
                <button
                  type='button'
                  class='btn btn-outline-warning'
                >{{button}} </button>
              </div>
            {{/with}}
          </div>
           <br />
          <div class='card'>
            {{#with collaboration}}
              <div class='card-body'>
                <div
                  class='display-4 text-muted text-end'
                >{{title}}
                </div>
                <h6 class='text-end'>{{sub_title}} </h6>
                <p class='text-end'>{{description}} </p>
                <button
                  type='button'
                  class='btn btn-outline-warning'
                >{{button}} </button>
              </div>
            {{/with}}
        {{/with}}
      </div>
    </div>
          </div>
    </div>
    <br>
     <br />
          <div class='card'>
            {{#with agreement}}
              <div class='card-body'>
                <div
                  class='display-5'
                >{{title}}
                </div>
                <div class='card'>
                  <div class='card-body'>
                    {{#with info}}
                    <h5>{{sub_title}} </h5>
                    <div class='card'>
                      <div class='card-body'>
                      <p>{{detail.text}} </p>
                      <button
                        type='button'
                        class='btn btn-success'
                      >{{detail.button}} </button>
                      </div>
                    </div>
                    {{/with}}
                  </div>
                </div>
              </div>
            {{/with}}
  {{/with}}
</main>