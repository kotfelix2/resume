<section class="ms-3">
{{> title data=make big=true }}
        <p>{{model}}, {{year}}, <button type="button" class="btn btn-secondary">Silver color</button> </p>
        <p>{{> title data='Features:' big=false }}</p>
        <div class="accordion" id="accordionExample">
                <div class="accordion-item">
                <h2 class="accordion-header">
                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        Interior
                </button>
                </h2>
                <div id="collapseOne" class="accordion-collapse collapse show" data-bs-parent="#accordionExample">
                <div class="accordion-body">
                        <div class="accordion accordion-flush" id="accordionFlushExample">
                                <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                                        Seats
                                </button>
                                </h2>
                                <div id="flush-collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                             <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>Material</th>
                                                        <th>Color</th>
                                                        <th>Heated</th>
                                                        <th>Ventilated</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.interior.seats.material}}
                                                </td>
                                                <td>
                                                        {{features.interior.seats.color}}
                                                </td>
                                                <td>
                                                        {{features.interior.seats.heated}}
                                                </td>
                                                <td>
                                                        {{features.interior.seats.ventilated}}
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>   
                                </div>
                                </div>
                                </div>

                                <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseTwo" aria-expanded="false" aria-controls="flush-collapseTwo">
                                        Dashboard
                                </button>
                                </h2>
                                <div id="flush-collapseTwo" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                        <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>Material</th>
                                                        <th>Color</th>
                                                        <th>Display</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.interior.dashboard.material}}
                                                </td>
                                                <td>
                                                        {{features.interior.dashboard.color}}
                                                </td>
                                                <td>
                                                        <h6>Type: <span class="badge bg-secondary">{{features.interior.dashboard.display.type}} </span></h6>
                                                        <h6>Size: <span class="badge bg-secondary">{{features.interior.dashboard.display.size}} </span></h6>
                                                        <h6>Resolution: <span class="badge bg-secondary">{{features.interior.dashboard.display.resolution}} </span></h6>
                                                        <h6>Touchscreen: <span class="badge bg-secondary">{{features.interior.dashboard.display.touchscreen}} </span></h6>
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>  
                                </div>
                                </div>
                                </div>

                                <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseThree" aria-expanded="false" aria-controls="flush-collapseThree">
                                        Audio
                                </button>
                                </h2>
                                <div id="flush-collapseThree" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                             <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>System</th>
                                                        <th>Speakers</th>
                                                        <th>Subwoofer</th>
                                                        <th>Bluetooth</th>
                                                        <th>USB</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.interior.audio.system}}
                                                </td>
                                                <td>
                                                        {{features.interior.audio.speakers}}
                                                </td>
                                                <td>
                                                        {{features.interior.audio.subwoofer}}
                                                </td>
                                                <td>
                                                        {{features.interior.audio.bluetooth}}
                                                </td>
                                                <td>
                                                        {{features.interior.audio.USB}}
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>   
                                </div>
                                </div>
                                </div>
                </div>
                </div>
                </div>
                <div class="accordion-item">
                <h2 class="accordion-header">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                        Exterior
                </button>
                </h2>
                <div id="collapseTwo" class="accordion-collapse collapse" data-bs-parent="#accordionExample">
                <div class="accordion-body">
                        <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                                        Wheels
                                </button>
                                </h2>
                                <div id="flush-collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                             <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>Size</th>
                                                        <th>Type</th>
                                                        <th>Color</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.exterior.wheels.size}}
                                                </td>
                                                <td>
                                                        {{features.exterior.wheels.type}}
                                                </td>
                                                <td>
                                                        {{features.exterior.wheels.color}}
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>   
                                </div>
                                </div>
                                </div>

                                <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseTwo" aria-expanded="false" aria-controls="flush-collapseTwo">
                                        Headlights
                                </button>
                                </h2>
                                <div id="flush-collapseTwo" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                             <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>Type</th>
                                                        <th>Brightness</th>
                                                        <th>Automatic</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.exterior.headlights.type}}
                                                </td>
                                                <td>
                                                        {{features.exterior.headlights.brightness}}
                                                </td>
                                                <td>
                                                        {{features.exterior.headlights.automatic}}
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>   
                                </div>
                                </div>
                                </div>

                                <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseThree" aria-expanded="false" aria-controls="flush-collapseThree">
                                        Sunroof
                                </button>
                                </h2>
                                <div id="flush-collapseThree" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                             <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>Type</th>
                                                        <th>Size</th>
                                                        <th>Automatic</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.exterior.sunroof.type}}
                                                </td>
                                                <td>
                                                        {{features.exterior.sunroof.size}}
                                                </td>
                                                <td>
                                                        {{features.exterior.sunroof.automatic}}
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>   
                                </div>
                                </div>
                                </div>
                </div>
                </div>
                </div>
                <div class="accordion-item">
                <h2 class="accordion-header">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                        Safety
                </button>
                </h2>
                <div id="collapseThree" class="accordion-collapse collapse" data-bs-parent="#accordionExample">
                <div class="accordion-body">
                        <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                                        Airbags
                                </button>
                                </h2>
                                <div id="flush-collapseOne" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                             <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>Front</th>
                                                        <th>Side</th>
                                                        <th>Knee</th>
                                                        <th>Rear</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.safety.airbags.front}}
                                                </td>
                                                <td>
                                                        {{features.safety.airbags.side}}
                                                </td>
                                                <td>
                                                        {{features.safety.airbags.knee}}
                                                </td>
                                                <td>
                                                        {{features.safety.airbags.rear}}
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>   
                                </div>
                                </div>
                                </div>

                                <div class="accordion-item">
                                <h2 class="accordion-header">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseTwo" aria-expanded="false" aria-controls="flush-collapseTwo">
                                        Assistance
                                </button>
                                </h2>
                                <div id="flush-collapseTwo" class="accordion-collapse collapse" data-bs-parent="#accordionFlushExample">
                                <div class="accordion-body">
                                             <table class="table table-striped">
                                                <thead>
                                                        <tr>
                                                        <th>Blind spot monitoring</th>
                                                        <th>Rear cross traffic alert</th>
                                                        <th>Lane departure warning</th>
                                                        <th>Adaptive cruise control</th>
                                                        <th>Collision warning</th>
                                                        </tr>
                                                </thead>
                                                <tbody>
                                                
                                                <tr>
                                                <td>
                                                        {{features.safety.assistance.blind_spot_monitoring}}
                                                </td>
                                                <td>
                                                        {{features.safety.assistance.rear_cross_traffic_alert}}
                                                </td>
                                                <td>
                                                        {{features.safety.assistance.lane_departure_warning}}
                                                </td>
                                                <td>
                                                        {{features.safety.assistance.adaptive_cruise_control}}
                                                </td>
                                                <td>
                                                        {{features.safety.assistance.collision_warning}}
                                                </td>
                                                </tr>
                                                
                                                </tbody>
                                                </table>   
                                </div>
                                </div>
                                </div>        
                </div>
                </div>
                </div>
        </div>
        <hr>
        <p>{{> title data='Engine:' big=false }}</p>
                <table class="table table-striped">
                        <thead>
                                <tr>
                                <th>Type</th>
                                <th>Displacement</th>
                                <th>Horsepower</th>
                                <th>Torque</th>
                                <th>Transmission</th>
                                </tr>
                        </thead>
                        <tbody>
                        <tr>
                        <td>
                                {{engine.type}}
                        </td>
                        <td>
                                {{engine.displacement}}
                        </td>
                        <td>
                                {{engine.horsepower}}
                        </td>
                        <td>
                                {{engine.torque}}
                        </td>
                        <td>
                                <h6>Type: <span class="badge bg-secondary">{{engine.transmission.type}} </span></h6>
                                <h6>Gears: <span class="badge bg-secondary">{{engine.transmission.gears}} </span></h6>
                        </td>
                        </tr>
                        </tbody>
                </table>
        <hr>
        <p>{{> title data='Fuel economy:' big=false }}
                </div>
                {{#with fuel_economy}}
                <ul class="list-group list-group-horizontal">
                        <li class="list-group-item">City: {{city}} </li>
                        <li class="list-group-item">Highway: {{highway}} </li>
                        <li class="list-group-item">Combined: {{combined}} </li>
                </ul>
                {{/with}}
        </div>
        </p>

        <hr>
        <p>{{> title data='Price:' big=false }}</p>
        <table class="table table-striped">
        <thead>
                <tr>
                <th>Base</th>
                <th>Destination</th>
                <th>Options</th>
                <th>Total</th>
                </tr>
        </thead>
        <tbody>
        
        <tr>
        <td>
                {{price.base}}
        </td>
        <td>
                {{price.destination}}
        </td>
         <td>
                <table class="table table-striped">
                        <thead>
                                <tr>
                                <th>Navigation</th>
                                <th>Moonroof</th>
                                <th>Premium paint</th>
                                </tr>
                        </thead>
                        <tbody>
                        
                        <tr>
                        <td>
                                {{price.options.navigation}}
                        </td>
                        <td>
                                {{price.options.moonroof}}
                        </td>
                        <td>
                                {{price.options.premium_paint}}
                        </td>
                        </tr>
                        </tbody>
                </table>
        </td>
         <td>
                {{price.total}}
        </td>
        </tr>
        
        </tbody>
        </table>
        <hr>
</section>